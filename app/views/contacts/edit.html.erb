<%= form_for @contact do |f| %>

	<%= f.label :email %>
	<%= f.text_field :email %><br>

	<%= f.label :phone %>
	<%= f.text_field :phone %><br>

	<%= f.label :fax %>
	<%= f.text_field :fax %><br>

	<%= f.label :address %>
	<%= f.text_field :address %><br>

	<%= f.label :fb %>
	<%= f.text_field :fb %><br>

	<%= f.label :twitter %>
	<%= f.text_field :twitter %><br>

	<%= f.label :open_from %>
	<%= f.text_field :open_from %><br>

	<%= f.label :open_to %>
	<%= f.text_field :open_to %><br>

	<%= f.label :open_hour %>
	<%= f.text_field :open_hour %><br>

	<%= f.label :close_hour %>
	<%= f.text_field :close_hour %><br>
 
	<%= f.submit 'save' %>
<% end -%>